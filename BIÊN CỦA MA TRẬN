#include<bits/stdc++.h>

using namespace std;

int main(){
	int n;
	cin >> n;
	while ( n > 0 ){
		int a;
		cin >> a;
		int arr[100][100];
		for ( int i = 1; i <= a; i++){
			for ( int j = 1; j <= a; j++){
				cin >> arr[i][j];
			}
		}
		for ( int i = 1; i <= a; i++){
			for ( int j = 1; j <= a; j++){
				if (i == 1 || i == a || j == 1 || j == a){
					cout << arr[i][j] << " ";
				}else{
					cout << "  ";
				}
			}
			cout << endl;
		}
		
		
	n--;
	}
}

