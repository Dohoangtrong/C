#include<bits/stdc++.h>

using namespace std;

int main(){
	int n;
	cin >> n;
	for ( int i = 1; i <= n; i++){
		int a, b, kt = 0;
		cin >> a >> b;
		for ( int i = 1; i < b; i++){
			if ( (a*i) % b == 1){
				kt = 1;
				cout << i << endl;
				break;
			}
		}
		if (kt == 0 )  cout << "-1" << endl;
	}
}

